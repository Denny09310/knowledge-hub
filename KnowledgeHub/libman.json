{
  "version": "3.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "flowbite@3.0.0-beta.1",
      "destination": "wwwroot/_lib/flowbite/",
      "files": [
        "dist/flowbite.min.js"
      ]
    },
    {
      "library": "anchor-js@5.0.0",
      "destination": "wwwroot/_lib/anchor-js/",
      "files": [
        "anchor.min.js"
      ]
    },
    {
      "library": "clipboard@2.0.11",
      "destination": "wwwroot/_lib/clipboard/",
      "files": [
        "dist/clipboard.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "prismjs@1.29.0",
      "destination": "wwwroot/_lib/prismjs/",
      "files": [
        "plugins/autoloader/prism-autoloader.min.js",
        "components/*.min.js",
        "themes/*.min.css"
      ]
    }
  ]
}